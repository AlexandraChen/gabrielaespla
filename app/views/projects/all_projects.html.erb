<div class="wrapper">
  <div class="container all-projects">
    <h1 class="item-title">Proyectos</h1>
    <div class= "project-wrapper">
      <div id="pins" class="transitions-enabled">
        <% @projects.each do |project| %>
          <div class="box panel panel-default" id="projects-box">
             <div class="panel-heading" id="img-wrapper">
              <%= link_to "",project_path(project.id), class:"project-overlay"  %>
              <%= image_tag(project.photos.first.img.url(:medium))%>
             </div>
             <div class="panel-body">
              <p class="item-subtitle"> <%= project.title %> </p>
            </div>
          </div>
        <% end %>
      </div>
    </div>
  </div>
</div>


